{
  "name": "fake-response",
  "version": "0.0.1",
  "description": "Mock proxy server",
  "keywords": [
    "mock",
    "mock-server",
    "proxy",
    "fake"
  ],
  "author": "Avin Lambrero<avin.github@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/avin/fake-response"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "bin": {
    "fake-response": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "prettier": "^2.8.1",
    "tsup": "^6.5.0"
  },
  "tsup": {
    "entry": [
      "src/index.ts",
      "src/cli.ts"
    ],
    "sourcemap": false,
    "dts": {
      "compilerOptions": {
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true
      }
    },
    "splitting": false,
    "clean": true,
    "format": [
      "esm",
      "cjs"
    ]
  },
  "dependencies": {
    "chokidar": "^3.5.3",
    "co-body": "^6.1.0",
    "esbuild": "^0.16.15",
    "lodash": "^4.17.21",
    "typescript": "^4.9.4"
  }
}
