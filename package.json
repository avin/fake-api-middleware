{
  "name": "fake-response",
  "version": "0.0.1",
  "description": "",
  "keywords": [
    "mock",
    "mock-server",
    "proxy",
    "fake"
  ],
  "author": "Avin Lambrero<avin.github@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/avin/fake-response"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "DEBUG=vite:plugin-mock-dev-server vite example --config ./example/vite.config.ts",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "prettier": "^2.8.1",
    "tsup": "^6.5.0"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "sourcemap": false,
    "dts": true,
    "splitting": false,
    "clean": true,
    "format": [
      "esm",
      "cjs"
    ]
  }
}
